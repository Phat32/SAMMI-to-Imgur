[insert_script]
function ImgurCrediantals(client_id, client_secret, refresh_token, file_name, btn) 
{	
	var formdata = new FormData()
	formdata.append("refresh_token", refresh_token)
	formdata.append("client_id", client_id)
	formdata.append("client_secret", client_secret)
	formdata.append("grant_type", "refresh_token")	

	var requestOptions = { 
							method: 'POST',
  							body: formdata,
  							redirect: 'follow'
						}

	fetch("https://api.imgur.com/oauth2/token", requestOptions)
  		.then(response => {
			switch(response.status){
				case 200:
					return ((response.json()));
					break;
				case 401:
					SAMMI.alert('Imgur Settings Error - Authentication Problem');
					return ('error'); 
					break;
				case 404:
					SAMMI.alert('Imgur Settings Error - No Response');
					return ('error'); 
					break;
				default:
					SAMMI.alert('Imgur Settings Error - Response Code: ' + response.status);
					return ('error');
					break;
			}
		})
  		.then(result => {
			  SAMMI.setVariable('client_id', client_id, 'ImgurSettings');
			  SAMMI.setVariable('access_token', result.access_token, 'ImgurSettings');			  
			  SAMMI.setVariable('file_name', file_name, 'ImgurSettings');
		})
  		.catch(error => SAMMI.alert(error));
}

function SaveToImgur(image, title, description, album, variable, btn) 
{		
	SAMMI.getVariable('access_token', 'ImgurSettings')
	  	.then(token => {
			var myHeaders = new Headers();
			myHeaders.append("Authorization", "Bearer " + token.Value);

			var formdata = new FormData();
			formdata.append("image", image);
			formdata.append("title", title);

			if (album != null)
			{
				formdata.append("album", album);
			}

			formdata.append("description", description);

			var requestOptions = {
				method: 'POST',
				headers: myHeaders,
				body: formdata,
				redirect: 'follow'
			};

			fetch("https://api.imgur.com/3/image", requestOptions)
			.then(response => {
				switch(response.status){
					case 200:
						return ((response.json()));
						break;
					case 401:
						SAMMI.alert('Imgur Error - Authentication Problem');
						return ('error'); 
						break;
					case 404:
						SAMMI.alert('Imgur Error - No Response');
						return ('error'); 
						break;
					default:
						SAMMI.alert('Imgur Error - Response Code: ' + response.status);
						return ((response.json()));
						break;
				}
			})
			.then(result => {
				if (result.success == 1) {
					if (variable !=  "") {
						SAMMI.setVariable(variable, 'https://imgur.com/' + result.data.id + '.png', btn)
					}
				}
				else {
					SAMMI.alert(result.data.error.message)
				}
			})
			.catch(error => SAMMI.alert('Imgur Error ' + error));
		})
	  	.catch(error => SAMMI.alert('Token Error ' + error));
}

function CreateAlbum(title, description, variable, variable2, btn) 
{		
	SAMMI.getVariable('access_token', 'ImgurSettings')
	  	.then(token => {
			var myHeaders = new Headers();
			myHeaders.append("Authorization", "Bearer " + token.Value);

			var formdata = new FormData();
			formdata.append("title", title);
			formdata.append("description", description);

			var requestOptions = {
				method: 'POST',
				headers: myHeaders,
				body: formdata,
				redirect: 'follow'
			};

			fetch("https://api.imgur.com/3/album", requestOptions)
			.then(response => {
				switch(response.status){
					case 200:
						return ((response.json()));
						break;
					case 401:
						SAMMI.alert('Imgur Error - Authentication Problem');
						return ('error'); 
						break;
					case 404:
						SAMMI.alert('Imgur Error - No Response');
						return ('error'); 
						break;
					default:
						SAMMI.alert('Imgur Error - Response Code: ' + response.status);
						return ((response.json()));
						break;
				}
			})
			.then(result => {			
				if (result.success == 1) {
					if (variable !=  "") {
						SAMMI.setVariable(variable, result.data.id, btn)
					}
					if (variable2 !=  "") {
						SAMMI.setVariable(variable2, "https://imgur.com/a/" + result.data.id, btn)
					}
				}
				else {
					SAMMI.alert(result.data.error)
				}	
			})
			.catch(error => SAMMI.alert('Imgur Error ' + error));
		})
	  	.catch(error => SAMMI.alert('Token Error ' + error));
}

function ShareAlbum(album, title, variable, btn) 
{		
	SAMMI.getVariable('access_token', 'ImgurSettings')
	  	.then(token => {
			var myHeaders = new Headers();
			myHeaders.append("Authorization", "Bearer " + token.Value);

			var formdata = new FormData();
			formdata.append("title", title);
			formdata.append("topic", "Funny");
			formdata.append("mature", "0");
			formdata.append("tags", "twitch,live stream,polaroid,obs,lioranboard");

			var requestOptions = {
				method: 'POST',
				headers: myHeaders,
				body: formdata,
				redirect: 'follow'
			};

			fetch("https://api.imgur.com/3/gallery/album/" + album, requestOptions)
			.then(response => {
				switch(response.status){
					case 200:
						return ((response.json()));
						break;
					case 401:
						SAMMI.alert('Imgur Error - Authentication Problem');
						return ('error'); 
						break;
					case 404:
						SAMMI.alert('Imgur Error - No Response');
						return ('error'); 
						break;
					default:
						SAMMI.alert('Imgur Error - Response Code: ' + response.status);
						return ((response.json()));
						break;
				}
			})
			.then(result => {	
				if (result.success == 1)		
				{
					if (variable !=  "") {
						SAMMI.setVariable(variable, "https://imgur.com/gallery/" + result.data.id, btn)
					}
				}				
				else 
				{
					SAMMI.alert(result.data.error)
				}
			})
			.catch(error => SAMMI.alert('Imgur Error ' + error));
		})
	  	.catch(error => SAMMI.alert('Token Error ' + error));
}

function GetAlbumImages(album, variable, btn) 
{		
	SAMMI.getVariable('client_id', 'ImgurSettings')
	.then(token => {
			var myHeaders = new Headers();
			myHeaders.append("Authorization", "Client-ID " + token.Value);
		var requestOptions = {
			method: 'GET',
			headers: myHeaders,
			redirect: 'follow'
		};

		fetch("https://api.imgur.com/3/album/" + album + "/images", requestOptions)
		.then(response => {
			switch(response.status){
				case 200:
					return ((response.json()));
					break;
				case 401:
					SAMMI.alert('Imgur Error - Authentication Problem');
					return ((response.json()));
					break;
				case 404:
					SAMMI.alert('Imgur Error - No Response');
					return ((response.json()));
					break;
				default:
					SAMMI.alert('Imgur Error - Response Code: ' + response.status);
					return ((response.json()));
					break;
			}
		})
		.then(result => {			
			if (result.success == 1) {
				if (variable !=  "") {
					SAMMI.setVariable(variable, result.data, btn)
				}
			}
			else {
				SAMMI.alert(result.data.error)
			}	
		})
		.catch(error => SAMMI.alert('Imgur Error ' + error));
	})
	.catch(error => SAMMI.alert('Token Error ' + error));
}

